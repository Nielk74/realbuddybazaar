{
  "name": "backendsqlite",
  "version": "1.0.0",
  "description": "backendsqlite is the ultimate backend solution",
  "main": "src/server.js",
  "scripts": {
    "setup": "npm install && npm run doc && npm run updatedb",
    "updatedb": "node src/util/updatedb.js",
    "doc": "node src/util/swagger.js",
    "setup:env": "cp .env.example .env && sed -i \"s/TOKENSECRET=/TOKENSECRET=$TOKENSECRET/g\" .env",
    "start": "npm run setup:env && npm run doc && npm run updatedb && node dist/server.js",
    "dev": "npm run updatedb && npm run doc && nodemon --watch src -e js,ts --exec \"ts-node src/server.js\"",
    "test:e2e": "npm run updatedb && npx jest --runInBand --testMatch='**/*.test.js'",
    "test:spec": "npx jest --runInBand --testMatch='**/*.spec.ts'",
    "test:integ": "npm run updatedb && npx jest --runInBand --testMatch='**/*.integ.ts'",
    "test": "npm run test:spec && npm run test:integ && npm run test:e2e",
    "build": "tsup src/server.js --format cjs --clean",
    "lint": "npx eslint src --ext .js,.ts"
  },
  "keywords": [
    "express",
    "backend",
    "backendsqlite"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-custom-error": "^1.8.4",
    "has-keys": "^0.1.0",
    "helmet": "^3.21.2",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^9.0.0",
    "mariadb": "^2.1.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "rotating-file-stream": "^1.4.6",
    "sequelize": "^5.19.2",
    "socket.io": "^4.6.1",
    "sqlite3": "^5.0.5",
    "swagger-autogen": "^2.18.8",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.2.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
